generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  full_name String   @default("UNKNOWN USER")
  age       Int
  email     String   @unique
  job       String
  isAdmin   Boolean?
}

model Post {
  id     Int     @id @default(autoincrement())
  title  String
  author String? @default("UNKNOWN AUTHOR")
}

